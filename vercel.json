{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://kalkulatorfinansowy.com.pl/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/feedback/:id",
      "destination": "/api/feedback/:id"
    }
  ]
}
